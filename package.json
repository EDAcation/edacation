{
    "name": "edacation",
    "description": "Library and CLI for interacting with Yosys and nextpnr.",
    "license": "MIT",
    "version": "0.1.1",
    "contributors": [
        "Danielle Huisman <danielle@huisman.me> (https://github.com/DanielleHuisman)"
    ],
    "repository": "github:EDAcation/edacation",
    "keywords": [
        "edacation",
        "cli",
        "yosys",
        "nextpnr"
    ],
    "type": "module",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "dist",
        "systemd",
        "LICENSE.md",
        "README.md",
        "package.json"
    ],
    "bin": "./dist/cli/index.js",
    "scripts": {
        "lint": "eslint",
        "clean": "rm -rf dist",
        "build": "yarn run clean && yarn run lint && tsc",
        "prepublish": "yarn run build",
        "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/cli/index.ts"
    },
    "dependencies": {
        "@types/node": "^18.14.6",
        "@types/yargs": "^17.0.22",
        "yargs": "^17.7.1",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.54.0",
        "@typescript-eslint/parser": "^5.54.0",
        "eslint": "^8.35.0",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-import": "^2.27.5",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    }
}
